</pre></td> --><td class="rouge-code"><pre> #include &lt;string&gt;
 #include &lt;vector&gt;
 #include &lt;map&gt;

 using namespace std;

 int solution(vector&lt;vector&lt;string&gt;&gt; clothes) {
     map&lt;string, int&gt; m; //ê° ì˜ìƒ ë³„ ê°œìˆ˜ë¥¼ ì €ì¥í•˜ê¸° ìœ„í•œ map
     vector&lt;string&gt; kinds; //ì˜ìƒì˜ ì¢…ë¥˜
     for (int i = 0; i &lt; clothes.size(); i++) {
         if (m[clothes[i][1]] &gt; 0) { //ì˜ìƒì˜ ì¢…ë¥˜ê°€ mapì— ì¡´ì¬í•˜ëŠ” ê²½ìš°
             m[clothes[i][1]]++;
         }
         else { //ì¡´ì¬í•˜ì§€ ì•ŠëŠ” ì‹ ê·œ
             kinds.push_back(clothes[i][1]);
             m[clothes[i][1]] = 1;
         }
     }
     int answer = 0;
        
     //ì•Œê³ ë¦¬ì¦˜ : ì´ì „ ê°’ + (ìƒˆ ì˜ìƒì˜ ê°œìˆ˜ + ìƒˆ ì˜ìƒì˜ ê°œìˆ˜ * ì´ì „ ê°’)
     //ì¦‰, ì´ì „ ê°’ + í•œê°œì§œë¦¬ + ì´ì „ ê°’ê³¼ì˜ ì¡°í•©
     for (int i = 0; i &lt; kinds.size(); i++) {
         answer = answer + (m[kinds[i]] + m[kinds[i]]*answer);
     }
     return answer;
 }
